                <xpath expr="/form/sheet/notebook/page[@name='other_info']/group/group[1]/field[@name='invoice_origin']" position="after">
                    <field name="sale_id" invisible="1"/>
                </xpath>
            </field>
        </record>
   
    </data>
</odoo>
