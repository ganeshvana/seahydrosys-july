<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	
		<record id="company_inherit_form_view" model="ir.ui.view">
			<field name="name">company.inherit.form.view</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form"/>
			<field name="arch" type="xml">
				<xpath expr="//notebook/page/group/group/field[@name='vat']" position="after">
					<field name="cin_no"/>
					<field name="ie_code"/>
					<field name="ad_code"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>