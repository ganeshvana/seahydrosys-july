<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_transaction_details_wizard_form" model="ir.ui.view">
            <field name="name">view.transaction.detailsd.form</field>
            <field name="model">transaction.details</field>
            <field name="arch" type="xml">
                <form>
                <group>
                    <group>
                            <field name="xls_file" readonly="1" filename="xls_filename"/>
                            <field name="xls_filename" readonly="1"/>
                    </group>
                    <group>
                            <field name ="start_date" placeholder="Select Date"/>
                            <field name ="end_date" placeholder="Select Date"/>
                            <field name="transaction_type" required="1"/>
                            
                    </group>
                </group>
                    <footer>
                        <button name="transaction_details_report" string="Export XLSX"
                                type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_gst_report" model="ir.actions.act_window">
        <field name="name">Transaction Details</field>
        <field name="res_model">transaction.details</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_transaction_details_wizard_form"/>
        <field name="target">new</field>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_view_types">list,form</field>
        </record>
    </data>
</odoo>