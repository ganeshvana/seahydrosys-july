<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>    
     <record id="resupply_report_view_form" model="ir.ui.view">
        <field name="name">resupply.report.view.form</field>
        <field name="model">resupply.report</field>
        <field name="arch" type="xml">
            <form>    
            	<group>
            		<group>
            			<field name="from_date"/>    
            		</group>
            		<group>
            			<field name="to_date"/>
            		</group>
            	</group>
            	<group>
            		<group>
            			<field name="xls_file" readonly="1" filename="xls_filename"/>
            			<field name="xls_filename" readonly="1"/>
            		</group>
            	</group>            
                <footer>
                    <button name="fuel_report" string="Export XLSX" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>               
            </form>
        </field>
    </record>

    <record id="action_resupply_report" model="ir.actions.act_window">
        <field name="name">Resupply Report</field>
        <field name="res_model">resupply.report</field>
        <field name="view_mode">form</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_id" ref="resupply_report_view_form"/>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
    
    <menuitem id="menu_resupply_report"
        action="action_resupply_report"
        name="Resupply Report"
        sequence="33" parent="purchase.menu_purchase_config"/>
        
        
    
          
    </data>     
</odoo>